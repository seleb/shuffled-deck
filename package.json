{
	"private": true,
	"name": "shuffled-deck",
	"version": "1.0.0",
	"description": "simple rng for a standard 52-card playing deck",
	"author": "Sean S. LeBlanc <sean.s.leblanc@gmail.com>",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/seleb/shuffled-deck.git"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			[
				"@semantic-release/github",
				{
					"successComment": false,
					"failTitle": false
				}
			],
			[
				"@semantic-release/git",
				{
					"assets": [
						"CHANGELOG.md",
						"package.json",
						"package-lock.json"
					]
				}
			]
		]
	},
	"scripts": {
		"postversion": "npm run build",
		"start": "vite --port 80",
		"typecheck": "tsc",
		"build": "vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"modern-normalize": "^3.0.1",
		"typescript": "^5.6.2",
		"vite": "^6.0.5"
	}
}
